# If backend uses default application.yml (8090), set 8090.
# If you run backend with --server.port=18080, keep 18080.
VITE_BACKEND_TARGET=http://localhost:18080
VITE_KEYCLOAK_TARGET=http://localhost:8081

# Leave empty for proxy-relative requests in dev
VITE_API_BASE=
VITE_KEYCLOAK_BASE=/keycloak
VITE_KEYCLOAK_REALM=autoguide
VITE_KEYCLOAK_CLIENT_ID=autoguide-api

# true = frontend blocks pages without token
# false = frontend can be used in security-disabled backend mode
VITE_REQUIRE_AUTH=false
